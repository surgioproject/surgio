{
  "name": "surgio",
  "description": "Generating rules for Surge, Clash, Quantumult like a PRO",
  "version": "2.19.0",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "bin": {
    "surgio": "bin/surgio.js"
  },
  "repository": "https://github.com/geekdada/surgio",
  "author": "Yihang Li <me@royli.dev>",
  "license": "MIT",
  "files": [
    "bin",
    "build",
    "hygen-template"
  ],
  "bugs": {
    "url": "https://github.com/geekdada/surgio/issues"
  },
  "homepage": "https://surgio.royli.dev",
  "scripts": {
    "dev": "run-s clean && tsc -p tsconfig.json --watch",
    "test": "run-s test:*",
    "test:lint": "eslint -c .eslintrc.js --ext .js,.ts .",
    "test:unit": "run-s build && ava",
    "coverage": "cross-env TS_NODE_FILES=true nyc ava",
    "clean": "rimraf build",
    "build": "run-s clean && tsc -p tsconfig.json",
    "prepublish": "run-s build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "pub": "np",
    "version": "npm run changelog && git add .",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "@royli/hygen": "^6.2.0",
    "@surgio/eslint-config-surgio": "^1.0.3",
    "@surgio/logger": "^1.1.0",
    "@types/eslint": "^7.2.0",
    "agentkeepalive": "^4.1.0",
    "ali-oss": "^6.7.0",
    "bluebird": "^3.7.2",
    "bytes": "^3.1.0",
    "chalk": "^4.0.0",
    "check-node-version": "^4.0.3",
    "common-bin": "^2.8.3",
    "cross-env": "^7.0.2",
    "date-fns": "^2.16.1",
    "detect-newline": "^3.1.0",
    "emoji-regex": "^10.1.0",
    "env2": "^2.2.2",
    "eslint": "^7.8.1",
    "espree": "^7.3.0",
    "execa": "^4.0.3",
    "filesize": "^9.0.0",
    "fs-extra": "^9.0.0",
    "get-port": "^5.1.0",
    "global-agent": "^3.0.0",
    "got": "^11.6.1",
    "inquirer": "^7.1.0",
    "ioredis": "^5.0.5",
    "joi": "^17.1.1",
    "listr": "^0.14.3",
    "lodash": "^4.17.19",
    "micromatch": "^4.0.5",
    "ms": "^2.1.3",
    "node-cache": "^5.1.2",
    "node-dir": "^0.1.17",
    "nunjucks": "^3.2.1",
    "ora": "^5.1.0",
    "query-string": "^7.1.1",
    "rimraf": "^3.0.0",
    "source-map-support": "^0.5.19",
    "update-notifier": "^6.0.0",
    "urlsafe-base64": "^1.0.0",
    "yaml": "^1.9.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^11.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/bluebird": "^3.5.30",
    "@types/bytes": "^3.1.0",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^9.0.13",
    "@types/hapi__joi": "^17.1.3",
    "@types/inquirer": "^7.3.0",
    "@types/listr": "^0.14.2",
    "@types/lodash": "^4.14.174",
    "@types/node": "^16",
    "@types/nunjucks": "^3.2.0",
    "@types/sinon": "^10.0.6",
    "@types/urlsafe-base64": "^1.0.28",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vuepress/plugin-docsearch": "^2.0.0-beta.26",
    "@vuepress/plugin-google-analytics": "^2.0.0-beta.26",
    "@vuepress/plugin-register-components": "^2.0.0-beta.26",
    "@vuepress/theme-default": "^2.0.0-beta.26",
    "@vuepress/utils": "^2.0.0-beta.25",
    "ava": "^4.0.1",
    "benchmark": "^2.1.4",
    "codecov": "^3.7.1",
    "coffee": "^5.3.0",
    "conventional-changelog-cli": "^2.0.31",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.0",
    "ini": "^1.3.5",
    "ioredis-mock": "^8.2.2",
    "lint-staged": "^12.1.7",
    "nock": "^13.0.0-beta.3",
    "np": "^6.3.1",
    "npm-debug-log-cleaner": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.1",
    "prettier": "^2.4.1",
    "sinon": "^12.0.1",
    "ts-node": "^10.4.0",
    "type-fest": "^2.3.4",
    "typescript": "^4.4.3",
    "vuepress": "^2.0.0-beta.26",
    "vuepress-plugin-sitemap2": "^2.0.0-beta.82"
  },
  "ava": {
    "failFast": false,
    "timeout": "30s",
    "extensions": [
      "ts",
      "js"
    ],
    "files": [
      "test/**/*.test.ts",
      "lib/**/*.test.ts"
    ],
    "require": [
      "ts-node/register",
      "./test/stub-axios.js"
    ]
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "reporter": [
      "html",
      "text-summary",
      "lcov"
    ]
  },
  "engines": {
    "node": ">=12.22.0"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.ts": "eslint --ext .ts"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm-debug-log-cleaner && lint-staged"
    }
  },
  "keywords": [
    "surgio",
    "surge",
    "quantumult",
    "clash",
    "shadowrocket"
  ]
}
